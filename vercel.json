{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/docs",
      "destination": "https://standns.mintlify.dev/docs"
    },
    {
      "source": "/docs/:match*",
      "destination": "https://standns.mintlify.dev/docs/:match*"
    }
  ]
}
