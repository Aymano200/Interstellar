{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "index.js",
      "permanent": true
    },
    {
      "source": "/docs(.*)",
      "destination": "https://standns.mintlify.app/faq",
      "permanent": true
    }
  ]
}
